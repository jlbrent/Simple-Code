import random

possible = "abcdefghikjklmnopqrstuvwxyz0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ!?@#$%^&*()"
pswrd = []

passlen = input("How long do you want your password to be? Please type a number and hit 'enter': ")

for x in range(int(passlen)):
	pswrd.append(random.choice(possible))
	
print("".join(pswrd))
